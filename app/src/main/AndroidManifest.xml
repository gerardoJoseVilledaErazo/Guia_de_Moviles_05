<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.orientacion_de_pantallas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Orientacion_de_pantallas">
        <activity
            android:name=".Calculadora"
            android:label="Calculadora"
            ></activity>
        <activity
            android:name=".LoginActivity"
            android:label="Time Plan Login" />
        <activity
            android:name=".ListConversationsActivity"
            android:label="@string/Conversations" />
        <activity
            android:name=".MostrarActivity"
            android:label="@string/Dev" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Guia 05 Orientacion de pantalla" />
    </application>

</manifest>